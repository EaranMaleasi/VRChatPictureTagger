{
  "SearchSettings": {
    "AdditionalPictureSearchPaths": [],
    "DontSearchVRChatFolder": false
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Debug"
      },
      {
        "Name": "File",
        "Args": {
          "path": "%LOCALAPPDATA%\\VRCPT\\Logs\\log.txt",
          "minimumLevel": "Error"
        }
      }

    ],
    "Enrich": [
      "FromLogContext",
      "WithDemystifiedStackTraces"
    ]
  }
}